<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.group2.dingmall.dao.OrderMapper">

    <insert id="addOneOrder">
        insert into `mall_order` (order_no,user_id,total_price)
        values (#{orderNo},#{userId},#{totalPrice})
    </insert>
</mapper>
