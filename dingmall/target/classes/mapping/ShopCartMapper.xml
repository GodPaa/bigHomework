<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">



<mapper namespace="com.group2.dingmall.dao.ShopCartMapper">


    <!-- 根据id查找购物项 -->
    <select id="getCartById" resultType="com.group2.dingmall.po.ShopCart">
        select * from shop_cart
        where book_id = #{bookId}
            and user_id = #{userId}
    </select>
</mapper>